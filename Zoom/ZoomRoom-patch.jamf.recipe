<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Description</key>
		<string>Updates the Policy for Zoom Rooms in the Meeting Rooms Site</string>
		<key>Identifier</key>
		<string>com.jackhenry.autopkg.digital-recipes.jamf.ZoomRoom-patch</string>
		<key>Input</key>
		<dict>
			<key>NAME</key>
			<string>Zoom Rooms</string>
			<key>POLICY_ENABLED</key>
			<string>false</string>
			<key>POLICY_CATEGORY</key>
			<string>Digital - Software</string>
			<key>POLICY_NAME</key>
			<string>Meeting Rooms - %NAME%</string>
			<key>POLICY_TEMPLATE</key>
			<string>JamfPolicyTemplate-MeetingRooms.xml</string>
		</dict>
		<key>MinimumVersion</key>
		<string>2.3</string>
		<key>ParentRecipe</key>
		<string>com.jackhenry.autopkg.digital-recipes.jamf.ZoomRoom-upload</string>
		<key>Process</key>
		<array>
			<dict>
				<key>Arguments</key>
				<dict>
					<key>policy_name</key>
					<string>%POLICY_NAME%</string>
					<key>policy_template</key>
					<string>%POLICY_TEMPLATE%</string>
					<key>replace_policy</key>
					<string>true</string>
				</dict>
				<key>Processor</key>
				<string>com.github.grahampugh.jamf-upload.processors/JamfPolicyUploader</string>
			</dict>
			<dict>
				<key>Processor</key>
				<string>com.github.grahampugh.jamf-upload.processors/JamfPolicyLogFlusher</string>
			</dict>
			<dict>
				<key>Processor</key>
				<string>com.github.grahampugh.recipes.postprocessors/LastRecipeRunResult</string>
			</dict>
		</array>
	</dict>
</plist>
